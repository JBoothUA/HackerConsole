	<!-- Special Move = open all tabs -->
	<Snippet>
		<Header>
			<Title>Special Move</Title>
			<Description>opens all of the tabs</Description>
			<Author Url="https://www.matrix.tools" Email="jesse@matrix.tools">JB</Author>
			<Version>1.0</Version>
			<ShellTypes>
				<ShellType>cmd</ShellType>
				<ShellType>powershell</ShellType>
			</ShellTypes>
		</Header>
		<Code>
			<![CDATA[$posh = New-Object -ComObject WScript.Shell
			$posh.SendKeys('^t')
			Sleep 1
			$posh.SendKeys('^{F2}')
			Sleep 1
			$posh.SendKeys('^{F3}')
			Sleep 1
			$posh.SendKeys('^{F4}')
			Sleep 1
			$posh.SendKeys('^{F5}')
			Sleep 1
			$posh.SendKeys('^{F6}')
			Sleep 1
			$posh.SendKeys('^{F7}')
			Sleep 1
			$posh.SendKeys('^{F8}')
			Sleep 1
			$posh.SendKeys('^{F9}')
			Sleep 1
			$posh.SendKeys('^{F10}')
			Sleep 1
			$posh.SendKeys('^{F11}')
			Sleep 1
			$posh.SendKeys('^{F12}')
]]> <!-- return of line, the command is launched after clicking OK -->
		</Code>
	</Snippet>


