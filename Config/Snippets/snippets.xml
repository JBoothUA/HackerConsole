	<!-- Special Move = open all tabs -->
	<Snippet>
		<Header>
			<Title>Special Move</Title>
			<Description>opens all of the tabs</Description>
			<Author Url="http://www.jessebooth.codes" Email="jesseboothua@gmail.com">JBoothUA</Author>
			<Version>1.0</Version>
			<ShellTypes>
				<ShellType>cmd</ShellType>
				<ShellType>powershell</ShellType>
			</ShellTypes>
		</Header>
		<Code>
			<![CDATA[$posh = New-Object -ComObject WScript.Shell
			$posh.SendKeys('^t')
			Sleep 1
			$posh.SendKeys('^{F2}')
			Sleep 1
			$posh.SendKeys('^{F3}')
			Sleep 1
			$posh.SendKeys('^{F4}')
