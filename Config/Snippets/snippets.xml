	<!-- Super Move -->
	<Snippet>
		<Header>
			<Title>Super Move</Title>
			<Description>opens all of the known tabs</Description>
			<Author Url="http://www.jessebooth.codes" Email="jesseboothua@gmail.com">JBoothUA</Author>
			<Version>1.0</Version>
			<ShellTypes>
				<ShellType>cmd</ShellType>
				<ShellType>powershell</ShellType>
			</ShellTypes>
		</Header>
		<Code>
			<![CDATA[$posh = New-Object -ComObject WScript.Shell
			$posh.SendKeys('^t')
			Sleep 1
			$posh.SendKeys('^{F2}')
			Sleep 1
			$posh.SendKeys('^{F3}')
			Sleep 1
			$posh.SendKeys('^{F4}')
			Sleep 1
			$posh.SendKeys('^{F5}')
			Sleep 1
			$posh.SendKeys('^{F6}')
			Sleep 1
			$posh.SendKeys('^{F7}')
			Sleep 1
			$posh.SendKeys('^{F8}')
			Sleep 1
			$posh.SendKeys('^{F9}')
]]> <!-- return of line, the command is launched after clicking OK -->
		</Code>
	</Snippet>


